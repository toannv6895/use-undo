{
  "name": "use-undo-v2",
  "version": "1.0.0",
  "description": "undo/redo functionality with React Hooks",
  "license": "MIT",
  "author": "Toan Nguyen",
  "homepage": "https://github.com/toannv6895/use-undo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toannv6895/use-undo.git"
  },
  "bugs": {
    "url": "https://github.com/toannv6895/use-undo/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "undo",
    "redo",
    "history",
    "state-management"
  ],
  "source": "src/index.ts",
  "main": "lib/use-undo-v2.js",
  "umd:main": "lib/use-undo-v2.umd.js",
  "module": "lib/use-undo-v2.m.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && microbundle -o lib/ --name use-undo-v2 --sourcemap false --no-compress",
    "prepublishOnly": "npm run build",
    "preversion": "npm run test:cov",
    "test": "jest",
    "test:cov": "jest --coverage --runInBand --forceExit",
    "test:watch": "jest --watch",
    "pre-commit": "pretty-quick --staged",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^27",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "microbundle": "^0.13.1",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  }
}
